genfscon sysfs /devices/platform/soc/9c0000.qcom,qupv3_i2c_geni_se/99c000.i2c/i2c-2/2-0012/leds/sar1/  u:object_r:sysfs_gamekey:s0
genfscon sysfs /class/leds/sar u:object_r:sysfs_gamekey:s0